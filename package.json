{
  "name": "table-select",
  "version": "0.0.0",
  "description": "Selecting row component in html tables",
  "keywords": [
    "js",
    "html",
    "table",
    "select"
  ],
  "author": {
    "name": "Matthias Fey",
    "email": "matthias.fey@tu-dortmund.de",
    "url": "http://rusty1s.github.io"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rusty1s/table-select.git"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ],
  "readmeFilename": "README.md",
  "bugs": {
    "url": "http://github.com/rusty1s/table-select/issues"
  },
  "main": "dist/table-select.js",
  "scripts": {
    "compile": "browserify src -t babelify -o dist/table-select.js",
    "uglify": "uglifyjs dist/table-select.js -o dist/table-select.min.js",
    "lint": "eslint src test",
    "tape": "browserify test -t babelify | tape-run | tap-spec",
    "start": "npm run compile && npm run uglify",
    "test": "npm run lint && npm run tape"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-plugin-lodash": "*",
    "babel-preset-es2015": "*",
    "babelify": "*",
    "browserify": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-plugin-react": "*",
    "tap-spec": "*",
    "tape": "*",
    "tape-run": "*",
    "uglify-js": "*"
  },
  "dependencies": {
    "custom-event": "^1.0.0",
    "lodash": "^4.6.0"
  }
}
